{
  "name": "ts-foursquare",
  "version": "0.0.0",
  "description": "A simple library for Foursquare API",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "test:coverage": "jest --config jestconfig.json --coverage && cat ./coverage/lcov.info | codacy-coverage",
    "test": "jest --config jestconfig.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/arnaud-zg/js-foursquare.git"
  },
  "keywords": [
    "ts-foursquare",
    "js-foursquare",
    "foursquare",
    "swarm",
    "4sq"
  ],
  "author": "Arnaud Zheng <arnaud-zg@users.noreply.github.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/arnaud-zg/js-foursquare/issues"
  },
  "homepage": "https://github.com/arnaud-zg/js-foursquare#readme",
  "dependencies": {
    "rxjs": "6.3.3"
  },
  "devDependencies": {
    "@types/jest": "23.3.13",
    "codacy-coverage": "3.4.0",
    "jest": "23.6.0",
    "prettier": "1.15.3",
    "standard-version": "4.4.0",
    "ts-jest": "23.10.5",
    "tslint": "5.12.1",
    "tslint-config-prettier": "1.17.0",
    "typescript": "3.2.4"
  }
}
